@article{isakssonAidInstitutionsLocal2023,
  title = {Aid and Institutions: {{Local}} Effects of {{World Bank}} Aid on Perceived Institutional Quality in {{Africa}}},
  shorttitle = {Aid and Institutions},
  author = {Isaksson, Ann-Sofie and Durevall, Dick},
